{
  "compilerOptions": {
    "baseUrl": "src/",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsxImportSource": "solid-js",
    "noEmit": true,
    "isolatedModules": true,
    "types": [
      "node",
      "vite/client"
    ],
  },
  "include": [
    "src",
  ],
  "exclude": []
}